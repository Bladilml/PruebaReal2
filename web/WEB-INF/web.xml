<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>prueba.servlets.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrarServlet</servlet-name>
        <servlet-class>prueba.servlets.registrarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>agregarProyectoServlet</servlet-name>
        <servlet-class>prueba.servlets.agregarProyectoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>provinciaServlet</servlet-name>
        <servlet-class>prueba.servlets.provinciaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>comunaServlet</servlet-name>
        <servlet-class>prueba.servlets.comunaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>regionServlet</servlet-name>
        <servlet-class>prueba.servlets.regionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mostrarProyectoServlet</servlet-name>
        <servlet-class>prueba.servlets.mostrarProyectoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutServlet</servlet-name>
        <servlet-class>prueba.servlets.logoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrarServlet</servlet-name>
        <url-pattern>/registrar.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>agregarProyectoServlet</servlet-name>
        <url-pattern>/agregarProyecto.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>provinciaServlet</servlet-name>
        <url-pattern>/provincia.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>comunaServlet</servlet-name>
        <url-pattern>/comuna.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>regionServlet</servlet-name>
        <url-pattern>/region.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mostrarProyectoServlet</servlet-name>
        <url-pattern>/mostrarProyecto.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutServlet</servlet-name>
        <url-pattern>/logout.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
